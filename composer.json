{
  "name": "wfeller/laravel-onboard",
  "description": "Track onboarding steps for users when they get setup in your app.",
  "license": "MIT",
  "authors": [
    {
      "name": "Caleb Porzio",
      "email": "caleb@fmtc.co"
    },
    {
      "name": "William",
      "homepage": "https://github.com/wfeller"
    }
  ],
  "require": {
    "php": "^7.2",
    "laravel/framework": "5.7.*"
  },
  "require-dev": {
    "orchestra/testbench": "3.7.*",
    "phpunit/phpunit": "^7.0"
  },
  "autoload": {
    "psr-4": {
      "WF\\Onboard\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "WF\\Onboard\\Test\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "WF\\Onboard\\OnboardServiceProvider"
      ]
    }
  }
}
