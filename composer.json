{
  "name": "calebporzio/onboard",
  "description": "Track onboarding steps for users when they get setup in your app.",
  "license": "MIT",
  "authors": [
    {
      "name": "Caleb Porzio",
      "email": "caleb@fmtc.co"
    }
  ],
  "require": {
    "php": "^7.2",
    "laravel/framework": "5.7.*"
  },
  "require-dev": {
    "orchestra/testbench": "3.7.*",
    "phpunit/phpunit": "^7.0"
  },
  "autoload": {
    "psr-4": {
      "Calebporzio\\Onboard\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Calebporzio\\Onboard\\Test\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Calebporzio\\Onboard\\OnboardServiceProvider"
      ],
      "aliases": {
        "Onboard": "Calebporzio\\Onboard\\OnboardFacade"
      }
    }
  }
}
